@model IEnumerable<PartyInvites.Models.GuestResponse>

@{
    Layout = null;
}


<!DOCTYPE html>
<html>
    <head>
        <title>Список подтвержденных</title>
    </head>

    <body>
        <div>
            <table>
                <thead>
                    <tr><th>Имя</th><th>Email</th></tr>
                </thead>
                <tbody>
                    @foreach(PartyInvites.Models.GuestResponse r in Model)
                    {
                        <tr>
                            <td>@r.Name</td>
                            <td>@r.Email</td>
                        </tr>
                    }
                </tbody>
           </table>
        </div>
    </body>
</html>